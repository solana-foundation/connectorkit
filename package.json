{
  "name": "connector-kit",
  "version": "0.0.1",
  "private": true,
  "description": "A connector kit for solana applications",
  "workspaces": [
    "packages/*",
    "apps/*",
    "examples/*",
    "tools/*"
  ],
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "type-check": "turbo type-check",
    "clean": "turbo clean",
    "format": "biome format --write .",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo build && changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@changesets/cli": "^2.29.7",
    "@types/d3-path": "^3.1.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.6.1",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "jsdom": "^26.1.0"
  },
  "packageManager": "pnpm@10.18.1"
}
