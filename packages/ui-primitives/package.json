{
  "name": "@connectorkit/ui-primitives",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./dialog-alpha": {
      "types": "./dist/dialog-alpha/index.d.ts",
      "import": "./dist/dialog-alpha/index.mjs",
      "require": "./dist/dialog-alpha/index.js"
    },
    "./dropdown-alpha": {
      "types": "./dist/dropdown-alpha/index.d.ts",
      "import": "./dist/dropdown-alpha/index.mjs",
      "require": "./dist/dropdown-alpha/index.js"
    }
  },
  "files": ["dist/**"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "@connectorkit/sdk": "workspace:*",
    "@wallet-ui/react": "^1.1.1"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@wallet-ui/react": "^1.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}