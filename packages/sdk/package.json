{
  "name": "@connector-kit/sdk",
  "version": "0.0.0",
  "description": "React hooks for Solana development - MVP with essential hooks only",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    }
  },
  "files": ["dist/**"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "biome lint src/",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "@tanstack/react-query": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "@tanstack/react-query": { "optional": true }
  },
  "dependencies": {
    "@connector-kit/connector": "workspace:*",
    "@solana-program/compute-budget": "^0.3.0",
    "@solana-program/system": "^0.7.0",
    "@solana-program/token": "^0.5.1",
    "@solana/transactions": "^2.0.0-preview.3",
    "@solana/keys": "^2.0.0-preview.3",
    "@solana/rpc-types": "^2.0.0-preview.3",
    "@solana/kit": "^2.0.0",
    "@wallet-standard/app": "^1.0.0",
    "@wallet-standard/base": "^1.0.0",
    "@wallet-standard/features": "^1.0.0",
    "@wallet-ui/core": "^1.1.1",
    "@wallet-ui/react": "^1.1.1",
    "@nanostores/react": "^1.0.0",
    "bs58": "^6.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "keywords": ["solana", "react", "hooks", "blockchain", "web3"],
  "license": "MIT"
}
