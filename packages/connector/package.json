{
  "name": "@connector-kit/connector",
  "version": "0.0.2",
  "description": "Headless wallet connector client and React provider built on Wallet Standard",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./headless": {
      "types": "./dist/headless.d.ts",
      "import": "./dist/headless.mjs",
      "require": "./dist/headless.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "build:analyze": "npm run build && node scripts/analyze-bundle.js",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "biome lint src/",
    "clean": "rm -rf dist",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "perf": "npm run build:analyze && npm run size"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "@solana-mobile/wallet-standard-mobile": "^0.3.0",
    "@wallet-standard/app": "^1.0.0",
    "@wallet-standard/base": "^1.0.0",
    "@wallet-ui/core": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "size-limit": "^11.0.0",
    "@size-limit/preset-big-lib": "^11.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "license": "MIT"
}