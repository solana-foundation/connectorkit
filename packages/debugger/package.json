{
    "name": "@solana/connector-debugger",
    "version": "0.1.0",
    "description": "Development debug panel for @solana/connector",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./react": {
            "types": "./dist/react.d.ts",
            "import": "./dist/react.mjs",
            "require": "./dist/react.js"
        }
    },
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "type-check": "tsc --noEmit",
        "lint": "prettier --check src/",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@solana/kit": "^4.0.0"
    },
    "peerDependencies": {
        "@solana/connector": "workspace:*",
        "react": ">=18.0.0"
    },
    "devDependencies": {
        "@solana/connector": "workspace:*",
        "@types/react": "^19.2.2",
        "tsup": "^8.5.0",
        "typescript": "^5.9.3"
    },
    "license": "MIT"
}
